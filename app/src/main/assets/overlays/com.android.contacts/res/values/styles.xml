<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DetailActivityTheme" parent="@*android:style/Theme.Material">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffdddddd</item>
        <item name="android:listViewStyle">@*com.android.contacts:style/ListViewStyle</item>
        <item name="android:actionBarStyle">@*com.android.contacts:style/ContactsActionBarStyle</item>
        <item name="android:actionOverflowButtonStyle">@*com.android.contacts:style/ContactsActionBarOverflow</item>
        <item name="android:homeAsUpIndicator">@*com.android.contacts:drawable/ic_menu_back</item>
        <item name="android:actionBarWidgetTheme">@*com.android.contacts:style/ContactsActionBarTheme</item>
        <item name="android:actionBarItemBackground">@*com.android.contacts:drawable/action_bar_item_background</item>
    </style>
    <style name="BackgroundOnlyTheme" parent="@*android:style/Theme.Material">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowNoDisplay">false</item>
    </style>
    <style name="PeopleTheme1" parent="@*android:style/Theme.Material">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffbbbbbb</item>
        <item name="android:colorForeground">#ffffffff</item>
        <item name="android:alertDialogTheme">@*com.android.contacts:style/ContactsAlertDialogTheme</item>
        <item name="android:colorPrimary">#ff1976d2</item>
        <item name="android:colorPrimaryDark">#ff0d47a1</item>
        <item name="android:colorAccent">#ff1976d2</item>
        <item name="android:statusBarColor">#ff0d47a1</item>
    </style>
    <style name="ContactsActionBarTheme" parent="@*android:style/Theme.Material">
        <item name="android:dropDownListViewStyle">@*com.android.contacts:style/ListViewDropdownStyle</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:popupMenuStyle">@*android:style/Widget.Holo.PopupMenu</item>
    </style>
    <style name="NonPhoneDialogTheme" parent="@*android:style/Theme.Material.Dialog" />
    <style name="ConfirmAddDetailDialogTheme" parent="@*android:style/Theme.Material.Dialog.MinWidth">
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="ContactEditorAccountsChangedActivityTheme" parent="@*android:style/Theme.Material.Dialog.NoActionBar.MinWidth">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffdddddd</item>
        <item name="android:listViewStyle">@*com.android.contacts:style/ListViewStyle</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:colorAccent">#ff1976d2</item>
    </style>
    <style name="ContactsAlertDialogTheme" parent="@*android:style/Theme.Material.Dialog">
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:colorControlActivated">#ff1976d2</item>
        <item name="android:colorAccent">#ff1976d2</item>
        <item name="android:statusBarColor">#ff0d47a1</item>
    </style>
    <style name="ContactsActionBarOverflowQP" parent="@*android:style/Widget.Material.ActionButton.Overflow">
        <item name="android:src">@*com.android.contacts:drawable/ic_more_vert</item>
        <item name="android:tint">#ffffffff</item>
    </style>
    <style name="ContactsActionBarDropDownStyle" parent="@*android:style/Widget.Holo.Spinner">
        <item name="android:background">@*com.android.contacts:drawable/ab_dropdown_navigation_item_background</item>
    </style>
    <style name="SelectableItem" parent="@*android:style/Theme.Material">
        <item name="android:background">?android:selectableItemBackground</item>
    </style>
    <style name="Theme.PhotoSelector" parent="@*android:style/Theme.Material">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="TextAppearanceMedium" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextAppearanceSmall" parent="@*android:style/TextAppearance.Small">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="ListViewStyle" parent="@*android:style/Widget.Material.ListView">
        <item name="android:overScrollMode">always</item>
    </style>
    <style name="ContactListFilterTheme" parent="@*com.android.contacts:style/PeopleTheme">
        <item name="android:listViewStyle">@*com.android.contacts:style/ListViewStyle</item>
    </style>


    <style name="CardView">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="ContactsActionBarOverflow" parent="@*android:style/Widget.Material.ActionButton.Overflow">
        <item name="android:src">@*com.android.contacts:drawable/ic_overflow_menu</item>
    </style>
</resources>
