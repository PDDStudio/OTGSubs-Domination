<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SettingsActionBarStyle" parent="@*android:style/Widget.Material.ActionBar">
        <item name="android:icon">@*android:color/transparent</item>
        <item name="android:background">#ff388e3c</item>
        <item name="android:displayOptions">homeAsUp|showHome|showTitle</item>
        <item name="android:contentInsetStart">@*com.google.android.apps.hangouts:dimen/settings_action_bar_content_inset_start</item>
    </style>
    <style name="Theme.Promo" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:colorPrimary">#ff388e3c</item>
        <item name="android:colorPrimaryDark">#ff1b5e20</item>
        <item name="android:colorAccent">#ff388e3c</item>
    </style>
    <style name="Theme.PhoneVerification" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:colorPrimary">#ff388e3c</item>
        <item name="android:colorPrimaryDark">#ff1b5e20</item>
        <item name="android:colorAccent">#ff388e3c</item>
    </style>
    <style name="Theme.DialerPromo" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:background">#ff388e3c</item>
        <item name="android:colorPrimaryDark">#ff1b5e20</item>
    </style>
    <style name="Theme.CallerId" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:colorPrimary">#ff1976d2</item>
        <item name="android:colorPrimaryDark">#ff0d47a1</item>
        <item name="android:colorAccent">#ff1976d2</item>
    </style>

    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@*android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText.Inverse" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Title" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Subtitle.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Subtitle" />
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />
    <style name="Base.TextAppearance.AppCompat.Inverse" parent="@*android:style/TextAppearance.Material" />
    <style name="Base.TextAppearance.AppCompat.Large.Inverse" parent="@*android:style/TextAppearance.Material.Large" />
    <style name="Base.TextAppearance.AppCompat.Medium.Inverse" parent="@*android:style/TextAppearance.Material.Medium" />
    <style name="Base.TextAppearance.AppCompat.Small.Inverse" parent="@*android:style/TextAppearance.Material.Small" />
    <style name="Platform.AppCompat" parent="@*android:style/Theme.Material">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.Material">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
    </style>
    <style name="NotificationPrimaryText">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="NotificationSecondaryText">
        <item name="android:textColor">#ffdddddd</item>
    </style>
    <style name="Theme.EmeraldSea.Settings" parent="@*android:style/Theme.Material">
        <item name="android:actionBarStyle">@*com.google.android.apps.hangouts:style/EsActionBar</item>
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:colorPrimary">#ff388e3c</item>
        <item name="android:colorPrimaryDark">#ff1b5e20</item>
        <item name="android:colorAccent">#ff388e3c</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">#ff1b5e20</item>
    </style>
    <style name="Theme.AlertDialog" parent="@*android:style/Theme.Material.Dialog.Alert" />

    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
</resources>
