        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="ContextualSearchCaptionTextView">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@*android:color/background_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">7.0dip</item>
        <item name="android:layout_marginRight">7.0dip</item>
        <item name="android:singleLine">true</item>
    </style>


    <style name="FullscreenWhite" parent="@*com.chrome.dev:style/Theme.AppCompat.Light">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="*com.chrome.dev:windowNoTitle">true</item>
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
