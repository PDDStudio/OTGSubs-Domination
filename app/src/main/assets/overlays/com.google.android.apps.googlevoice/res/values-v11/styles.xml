<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LightThemeSelector" parent="@*android:style/Theme.Holo" />
    <style name="LightThemeNoActionBar" parent="@*android:style/Theme.Holo.NoActionBar">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TranslucentThemeNoActionBar" parent="@*android:style/Theme.Holo.NoActionBar">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="SettingsTheme" parent="@*android:style/Theme.Holo" />
    <style name="DialogTheme" parent="@*android:style/Theme.Holo.Dialog" />


    <style name="Oob.Dialog" parent="@*android:style/Theme.Holo.Dialog">
        <item name="android:textSize">@*com.google.android.apps.googlevoice:dimen/text_size_20</item>
        <item name="android:textColor">#ffdddddd</item>
    </style>
    <style name="Platform.V11.AppCompat.Light" parent="@*android:style/Theme.Holo" />
</resources>
