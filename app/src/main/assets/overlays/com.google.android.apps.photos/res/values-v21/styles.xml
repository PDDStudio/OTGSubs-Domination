<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Base.TextAppearance.AppCompat.Inverse" parent="@*android:style/TextAppearance.Material" />
    <style name="Base.TextAppearance.AppCompat.Large.Inverse" parent="@*android:style/TextAppearance.Material.Large" />
    <style name="Base.TextAppearance.AppCompat.Medium.Inverse" parent="@*android:style/TextAppearance.Material.Medium" />
    <style name="Base.TextAppearance.AppCompat.Small.Inverse" parent="@*android:style/TextAppearance.Material.Small" />

    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Subtitle.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Subtitle" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Title" />


    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText.Inverse" parent="@*android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@*android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@*android:style/Widget.Material.PopupMenu" />


    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.Material">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:statusBarColor">@*android:color/background_dark</item>
        <item name="android:textColorPrimaryInverse">#ffffffff</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
        <item name="android:colorPrimary">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>
    </style>

    <style name="Theme.Photos.Button.Light" parent="@*android:style/Widget.Material.Button">
        <item name="android:theme">@*com.google.android.apps.photos:style/PhotosSettingButtonTheme</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:paddingLeft">@*com.google.android.apps.photos:dimen/photos_theme_envelope_components_padding</item>
        <item name="android:paddingRight">@*com.google.android.apps.photos:dimen/photos_theme_envelope_components_padding</item>
        <item name="android:minWidth">88.0dip</item>
        <item name="android:minHeight">48.0dip</item>
    </style>

    <style name="CPE.Theme.OverlayActionBar.Dialog" parent="@*android:style/Theme.Material.Dialog.Alert">
        <item name="android:colorPrimary">#ff1976d2</item>
        <item name="android:colorPrimaryDark">#ff0d47a1</item>
        <item name="android:colorAccent">#ff1976d2</item>
    </style>
    <style name="JoinBannerButtonTheme">
        <item name="android:colorButtonNormal">@*android:color/button_material_dark</item>
    </style>
    <style name="PhotosSettingButtonTheme">
        <item name="android:colorButtonNormal">@*android:color/button_material_dark</item>
    </style>
    <style name="SkipTrashDialogNegativeBtnStyle" parent="@*android:style/Widget.Material.Button.Borderless.Colored">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="SkipTrashDialogPositiveBtnStyle" parent="@*android:style/Widget.Material.Button.Borderless.Colored">
        <item name="android:textColor">#ffd32f2f</item>
    </style>
    <style name="Theme.Photos.Backup.Settings.Dialog" parent="@*android:style/Theme.Material.Dialog.Alert">
        <item name="android:colorPrimary">#ff1976d2</item>
        <item name="android:colorPrimaryDark">#ff0d47a1</item>
        <item name="android:colorAccent">#ff1976d2</item>
    </style>
    <style name="Theme.Photos.Dialog" parent="@*android:style/Theme.Material.Dialog.Alert">
        <item name="android:colorPrimary">#ff1976d2</item>
        <item name="android:colorPrimaryDark">#ff0d47a1</item>
        <item name="android:colorAccent">#ff1976d2</item>
    </style>
    <style name="UrawTheme" parent="@*android:style/Theme.Material">
        <item name="android:colorControlActivated">#ff1976d2</item>
    </style>
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
    <style name="Theme.CPE.Dialog" parent="@*android:style/Theme.Material.Dialog.Alert">
        <item name="android:colorPrimary">#ff1976d2</item>
        <item name="android:colorPrimaryDark">#ff0d47a1</item>
        <item name="android:colorAccent">#ff1976d2</item>
    </style>


    <style name="DialogTitle" parent="@*android:style/TextAppearance.Material.DialogWindowTitle">
        <item name="android:textColor">#ffffffff</item>
    </style>
</resources>
